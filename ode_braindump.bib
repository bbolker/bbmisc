@article{hookerCollocInfer2016,
  title = {{{CollocInfer}}: {{Collocation Inference}} in {{Differential Equation Models}}},
  shorttitle = {{{CollocInfer}}},
  author = {Hooker, Giles and Ramsay, James O. and Xiao, Luo},
